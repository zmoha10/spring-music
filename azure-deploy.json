
{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.1",
    "parameters": {
        "stringToTest": {
            "type": "string",
            "defaultValue": "OneTwoThree"
        },
        "objectToTest": {
            "type": "array",
            "defaultValue": [
              {
                "name": "customer-profile",
                "throughput": 400,
                "ttl": -1,
                "sharedKey": [
                  "/aggregateId"
                ],
                "uniqueKey": [
                  "/_id"
                ]
              },
              {
                "name": "test",
                "throughput": 1000,
                "ttl": -1,
                "sharedKey": [
                  "/aggregateId"
                ],
                "uniqueKey": [
                  "/_id"
                ]
              }
            ]
        },
        "arrayToTest": {
            "type": "array",
            "defaultValue": ["one", "two", "three"]
        },
        "consistencyMaxLagInSeconds": {
          "type": "int",
          "defaultValue": 5,
          "minValue": 5,
          "maxValue": 86400
        },
        "consistencyMaxLagInOperations": {
          "type": "int",
          "defaultValue": 100000,
          "minValue": 10,
          "maxValue": 1000000
        }
    },
    "variables": {
      "testString": "[replace(replace(string(parameters('objectToTest')),'\\','\\\\'),'\"','\\\"')]"
    },
    "resources": [
    ],
    "outputs": {
        "stringTrue": {
            "type": "bool",
            "value": "[contains(parameters('stringToTest'), 'e')]"
        },
        "stringFalse": {
            "type": "bool",
            "value": "[contains(parameters('stringToTest'), 'z')]"
        },
        "arrayTrue": {
            "type": "bool",
            "value": "[contains(parameters('arrayToTest'), 'three')]"
        },
        "arrayFalse": {
            "type": "bool",
            "value": "[contains(parameters('arrayToTest'), 'four')]"
        },
        "objectToString": {
            "type": "string",
            "value": "[variables('testString')]"
        }
    }
}
